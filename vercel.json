{
  "version": 2,
  "builds": [
    {
      "src": "api/send-booking.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "hero-image.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "about-photo.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "logos/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/send-booking",
      "dest": "api/send-booking.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

